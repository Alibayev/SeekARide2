@model System.Collections.Generic.ICollection<SeekARide.Models.Location>
  @if (Model.Count == 0) {
	  <span>There are no addresses in the Address Book</span>
  }
  else {
	  <table>
		  <thead>
		  <tr>
			  <th>Street Address</th>
			  <th>City</th>
			  <th>State</th>
			  <th>Zip</th>
		  </tr>
		  </thead>
		  <tbody>
		  @foreach (var item in Model) {

			  <tr>
				  <td>
					  @Html.Encode(item.StreetAddress)
				  </td>
				  <td>
					  @Html.Encode(item.State)
				  </td>
				  <td>
					  @Html.Encode(item.City)
				  </td>
				  <td>
					  @Html.Encode(item.ZipCode)
				  </td>
				  <td>
					  <input type="button" value="Populate" onclick="populateData('@Html.Encode(item.StreetAddress)', '@Html.Encode(item.City)', '@Html.Encode(item.State)', '@Html.Encode(item.ZipCode)');"/>
				  </td>
			  </tr>

		  }
		  </tbody>
	  </table>
  }